# Son1kVers3 - Variables de Entorno
# Copia este archivo como .env y configura tus valores

# Configuración de la API
SECRET_KEY=son1k_ultra_secret_key_2024
NODE_SERVER_URL=http://localhost:3001
PYTHON_SERVER_URL=http://localhost:8000

# Base de datos
DATABASE_URL=sqlite:///./son1k.db

# Suno AI - Cookies de autenticación
SUNO_COOKIE=tu_cookie_suno_aqui
SUNO_COOKIE_2=cookie_adicional_suno
SUNO_COOKIE_3=tercera_cookie_suno

# Configuración de JWT
JWT_SECRET_KEY=son1k_jwt_secret_2024
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configuración de CORS
ALLOWED_ORIGINS=http://localhost:8000,http://localhost:3000,http://localhost:3001

# Configuración de logging
LOG_LEVEL=INFO
LOG_FILE=logs/son1k.log

# Configuración de Railway (para producción)
RAILWAY_ENVIRONMENT=production
PORT=8000

# Configuración de Ollama (opcional)
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama2

# Configuración de Waves (opcional)
WAVES_PLUGIN_PATH=/Applications/Waves
WAVES_LICENSE_PATH=/Applications/Waves/Licenses

# Configuración de Studio One (opcional)
STUDIO_ONE_PATH=/Applications/PreSonus/Studio One
STUDIO_ONE_PLUGIN_PATH=/Applications/PreSonus/Studio One/Plugins

# Configuración de email (opcional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=tu_email@gmail.com
SMTP_PASSWORD=tu_password_app

# Configuración de Redis (opcional)
REDIS_URL=redis://localhost:6379

# Configuración de Celery (opcional)
CELERY_BROKER_URL=redis://localhost:6379
CELERY_RESULT_BACKEND=redis://localhost:6379







